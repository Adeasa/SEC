TARGET = built-in.o
OBJS = TestAgent.o
CFLAGS = -q64 -qmaxmem=16396 -qcpluscmt -qalign=power -qenum=int -fPIC -O
LD = ld -b64

all : $(OBJS)	
	$(LD) -r $(OBJS) -o $(TARGET)

clean :
	rm -rf *.o $(TARGET)
